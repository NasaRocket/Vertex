{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "$rust-wasm/*": ["./rust-wasm/pkg/*"]
    },
    "esModuleInterop": true
  },
  "include": ["./src/**/*"]
}